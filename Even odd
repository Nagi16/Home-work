import java.util.Scanner;

import java.util.*;

 // Compiler version JDK 11.0.2

 public class Even_odd

 {

   public static void main(String args[])

   { 

    int n;

    Scanner c = new Scanner(System.in);

    n=c.nextInt();

    ((n&1)==0)? System.out.println("even"): System.out.println("odd")

   }

 }

